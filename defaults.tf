# These are default values which shouldn't need to be changed by users
# Assign a number to each AZ letter used in our configuration
variable "az_number" { # tflint-ignore: terraform_unused_declarations
  type = map(any)
  default = {
    a = 1
    b = 2
    c = 3
    d = 4
    e = 5
    f = 6
    g = 7
    h = 8
  }
}

